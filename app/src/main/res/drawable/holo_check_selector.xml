<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_enabled="false"
        android:state_checked="true"
        android:drawable="@drawable/holo_check_disabled_on" />

    <item
        android:state_pressed="true"
        android:state_checked="true"
        android:drawable="@drawable/holo_check_pressed_on" />

    <item
        android:state_pressed="true"
        android:state_checked="false"
        android:drawable="@drawable/holo_check_pressed_off" />

    <item
        android:state_enabled="false"
        android:drawable="@drawable/holo_check_disabled_off" />

    <item
        android:state_checked="true"
        android:drawable="@drawable/holo_check_on" />

    <item
        android:drawable="@drawable/holo_check_off" />
</selector>